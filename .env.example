# Supabase API config, found at /settings/api
SUPABASE_URL=https://<YOUR_SUPABASE_SLUG>.supabase.co
SUPABASE_ANON_KEY=<YOUR_SUPABASE_ANON_KEY>
SUPABASE_SERVICE_ROLE_KEY=<YOUR_SUPABASE_SERVICE_ROLE_KEY>

# Supabase DB config (optional), found at /settings/database
# If provided, migrations are automatically run on startup of Docker container
DATABASE_URL=postgresql://postgres:<YOUR_SUPABASE_DB_PASSWORD>@<YOUR_SUPABASE_DB_HOST>:5432/postgres
